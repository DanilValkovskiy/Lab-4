#include <iostream>

// Процедура лівого циклічного зсуву
void ShiftLeft3(double &A, double &B, double &C) {
    double temp = A;
    A = B;
    B = C;
    C = temp;
}

int main() {
    // Вхідні дані для першого набору
    double A1 = 1.0, B1 = 2.0, C1 = 3.0;

    // Виклик процедури для першого набору
    ShiftLeft3(A1, B1, C1);

    // Вивід результатів для першого набору
    std::cout << "Після зсуву для першого набору: A1=" << A1 << ", B1=" << B1 << ", C1=" << C1 << std::endl;

    // Вхідні дані для другого набору
    double A2 = 4.0, B2 = 5.0, C2 = 6.0;

    // Виклик процедури для другого набору
    ShiftLeft3(A2, B2, C2);

    // Вивід результатів для другого набору
    std::cout << "Після зсуву для другого набору: A2=" << A2 << ", B2=" << B2 << ", C2=" << C2 << std::endl;

    return 0;
}
